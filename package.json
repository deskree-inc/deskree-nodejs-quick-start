{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "A template project to express js apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deskree-inc/express-boilerplate.git"
  },
  "author": "Deskree Inc.",
  "license": "EULA",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "clean": "rimraf lib --progress",
    "serve": "npm run lint && tsc && node lib/src/index.js",
    "docs": "npx typedoc src/controllers/*.ts src/models/*.ts src/interfaces/*.ts src/*.ts --out wiki --plugin typedoc-plugin-markdown --theme ./node_modules/typedoc-github-wiki-theme/dist",
    "test": "jest --coverage",
    "test:controllers": "jest --testPathPattern=controllers/",
    "test:unit": "jest --testPathPattern=unit/",
    "postman": "postman-generator --folder src/controllers --outputDir ./ --integrationName Deskree Integration --baseURL https://user.deskree.com/api/v1"
  },
  "pre-commit": [
    "lint"
  ],
  "engines": {
    "node": "16"
  },
  "main": "lib/src/index.js",
  "dependencies": {
    "@deskree-inc/logger": "^1.0.7",
    "@deskree-inc/postman-collection-generator": "^1.0.24",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "supertest": "^6.1.3",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.5",
    "typedoc": "^0.21.4",
    "typedoc-github-wiki-theme": "^0.5.0",
    "typedoc-plugin-markdown": "^3.10.3",
    "typescript": "^4.3.5"
  },
  "private": true
}
